<% #= console if current_user.is_admin? %>
<h2>Server</h2>
<div class="row">
  <div class="col-md-6">
    <h3>Rooms</h3>
    <pre id="rooms_events">
    </pre>
    <div id="rooms">
      <%= render @rooms %>
    </div>
  </div>
  <div class="col-md-6">
    <h3>Messages</h3>
    <pre id="messages_events">
    </pre>
    <div id="messages">
      <%= render @messages %>
    </div>
  </div>
</div>
